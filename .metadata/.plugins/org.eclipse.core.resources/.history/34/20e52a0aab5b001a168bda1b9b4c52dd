		//int sum = n*(n+1)*(2*n-1)/6;
		int sum = (2*n+1)*n*(n+1)/6;
		for(i=1;i<=3;i++){
		    s = i+ "^2+";
		   // System.out.print(s);
		}
		//System.out.print(i+"^2=" + sum);

		System.out.print( sum);


