
public class MatrixAddition {

	public static void main(String[] args) {
		int a[][] = {{1,2,3},{4,5,6},{7,8,9}};
		int b[][] = {{1,2,3},{4,5,6},{7,8,9}};
	
		matrix(a,b);
	
	}
	
	/**
	 * 2d matrix addition
	 */

	public static void matrix(int a[][],int b[][]) {
		
		for(int i=0;i<a.length;i++){
			for(int j = 0;j<b.length;j++){
				int c[][] = new int[3][3];
				c[i][j] = a[i][j]+b[i][j];
			}
		}

		
	}
	
}
