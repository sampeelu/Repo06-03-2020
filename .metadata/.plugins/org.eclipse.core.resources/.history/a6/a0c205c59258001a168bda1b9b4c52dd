
public class RemoveDuplicate {

	public static void main(String[] args) {

	}
	
	public static void removeDuplicate() {
		
		String str  = "sambhawjain";
		char []arr = str.toCharArray();
		for(int i=0;i<arr.length;i++){
			for(int j = 0;j<arr.length;j++){
				if(arr[i]==arr[j])
					arr[i]=0;
			}
		}
		
	}

}
