
public class Patterns {

	public static void main(String[] args) {
		t1();
		System.out.println("......................................");



	}

	public static void t1() {
		for(int i=1;i<=5;i++) {		
			for(int k=5;k>=i;k--) {
				System.out.print("  ");
			}
			for(int j=1;j<=2*i-1;j++) {
				
					System.out.print(" *");
				
			}
			System.out.println();
		}

	}
	
	public static void t2() {
		
	}


} 



