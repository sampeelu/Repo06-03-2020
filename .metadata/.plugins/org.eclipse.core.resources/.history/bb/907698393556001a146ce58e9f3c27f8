
public class Patterns {

	public static void main(String[] args) {
		System.out.println("......................................");



	}

	public static void t1() {
		for(int i=1;i<=5;i++) {		
			for(int k=5;k>=i;k--) {
				System.out.print("  ");
			}
			for(int j=1;j<=2*i-1;j++) {
				if(i==j||i==5||i+j==5)
					System.out.print(" *");
				else
					System.out.print("");
			}
			System.out.println();
		}

	}


} 



